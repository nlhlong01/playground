{
  "name": "ml-playground-prototype",
  "version": "2016.3.10",
  "description": "An extension of Deep Playground for other ML algorithms.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prep": "copyfiles analytics.js dist && concat node_modules/material-design-lite/material.min.js node_modules/seedrandom/seedrandom.min.js > dist/lib.js",
    "build-css": "concat node_modules/material-design-lite/material.min.css src/styles.css > dist/bundle.css",
    "build-html": "copyfiles -f src/index.html dist",
    "build-js": "webpack --config webpack.prod.js",
    "build": "npm run clean && npm run prep && npm run build-js && npm run build-css && npm run build-html",
    "serve": "npx serve dist/",
    "start": "npm run build && npm run serve",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/d3": "^3.5.43",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "concat": "^1.0.3",
    "concurrently": "3.1.0",
    "copyfiles": "1.0.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.4.0",
    "rimraf": "2.5.4",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "tsify": "^4.0.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "material-design-lite": "^1.3.0",
    "seedrandom": "^2.4.3"
  }
}
