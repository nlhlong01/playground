{
  "name": "ml-playground",
  "version": "2020.11.27",
  "description": "An extension of Deep Playground for multiple ML algorithms.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test:run": "cypress run",
    "test:open": "cypress open",
    "test": "start-server-and-test start http-get://localhost:5000 test:run",
    "test:dev": "start-server-and-test start http-get://localhost:5000 test:open",
    "test:coverage": "nyc npm run test",
    "test:run:coverage": "nyc npm run test:run",
    "deploy": "npm run build && rimraf docs/* && copyfiles -f dist/* docs"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@types/d3": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.4.0",
    "css-loader": "^4.2.1",
    "css-minimizer-webpack-plugin": "^1.1.2",
    "cypress": "^5.0.0",
    "cypress-file-upload": "^4.1.1",
    "eslint": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "extract-loader": "^5.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "nyc": "^15.1.0",
    "rimraf": "2.5.4",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2",
    "worker-loader": "^3.0.5"
  },
  "dependencies": {
    "custom-ml-cart": "github:nlhlong01/decision-tree-cart",
    "d3": "^6.3.0",
    "jsonschema": "^1.4.0",
    "material-design-lite": "^1.3.0",
    "ml-random-forest": "github:nlhlong01/random-forest",
    "seedrandom": "^2.4.3"
