{
  "name": "ml-playground-prototype",
  "version": "2016.3.10",
  "description": "An extension of Deep Playground for other ML algorithms.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prep": "copyfiles -f src/analytics.js dist && concat node_modules/seedrandom/seedrandom.min.js > dist/lib.js",
    "build": "npm run clean && npm run prep && webpack --mode production --devtool false",
    "start": "npm run clean && npm run prep && webpack-dev-server",
    "test": "",
    "test:coverage": ""
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/d3": "^3.5.43",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "concat": "^1.0.3",
    "concurrently": "3.1.0",
    "copyfiles": "1.0.0",
    "css-loader": "^4.2.1",
    "css-minimizer-webpack-plugin": "^1.1.2",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "rimraf": "2.5.4",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "tsify": "^4.0.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "material-design-lite": "^1.3.0",
    "seedrandom": "^2.4.3"
  }
}
